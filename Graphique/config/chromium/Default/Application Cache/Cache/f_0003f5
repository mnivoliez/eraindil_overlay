<script type="text/javascript">
window.onerror = function(msg, url, line) {
	if (typeof(OFFLINE) != 'undefined' && OFFLINE) {
		return false;
	}

	var err = {};
	err.STATIC_VERSION = '00201256';
	err.DESC = msg;
	err.URL = url;
	err.LINE = line;
	err.BROWSER_URL = document.location.href;
	//err.USER_AGENT = navigator.userAgent;
	err.BROWSER = BrowserDetect.browser;
	err.BROWSER_VER = BrowserDetect.version;
	err.OS = BrowserDetect.OS;

	$.ajax({type: 'POST', url: ('http://www.deezer.com/ajax/error.php'), data: $.param(err)});

	console.log('onerror', msg, url, line);
	return true;
};
</script>
