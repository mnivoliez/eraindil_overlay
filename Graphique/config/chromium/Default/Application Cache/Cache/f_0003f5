<script type="text/javascript">
var gatekeeper = {
	
	features : {"signin_googleplus":true,"artist_spellcheck":false,"mobile_offline_expiration":false,"hotkeys":true,"facebook_og_action":true},
	isAllowed : function(feature) {
		if (typeof(gatekeeper.features[feature]) == 'undefined') {
			return false;
		}
		return gatekeeper.features[feature];
	}
};
